{
  "include": [
    "src",
    "shared"
  ],
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "~/shared/*": [
        "./shared/*"
      ],
      "~/models/*": [
        "./models/*"
      ]
    },
    "allowImportingTsExtensions": true
  }
}
