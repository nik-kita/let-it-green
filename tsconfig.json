{
  "include": [
    "src",
    "shared"
  ],
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "@project/shared/*": [
        "./shared/*"
      ],
      "@project/models/*": [
        "./models/*"
      ]
    },
    "allowImportingTsExtensions": true
  }
}
